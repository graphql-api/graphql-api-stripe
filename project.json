{
  "root": "libs/graphql/stripe",
  "sourceRoot": "libs/graphql/stripe/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/graphql/stripe",
        "main": "libs/graphql/stripe/src/index.ts",
        "tsConfig": "libs/graphql/stripe/tsconfig.lib.json",
        "assets": ["libs/graphql/stripe/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/graphql/stripe/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/graphql/stripe"],
      "options": {
        "jestConfig": "libs/graphql/stripe/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
