const withExpand = typename => next => async (root, args, context, info) =>
  next(root, { ...args, expand: expand[typename] }, context, info);

export const expandResolvers = {
  "Query.retrieveDispute": [withExpand("Dispute")],
  "Query.listDisputes": [withExpand("DisputeList")],
  "Mutation.updateDispute": [withExpand("Dispute")],
  "Mutation.closeDispute": [withExpand("Dispute")]
};
