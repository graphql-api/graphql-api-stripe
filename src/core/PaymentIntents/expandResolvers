const withExpand = typename => next => async (root, args, context, info) =>
  next(root, { ...args, expand: expand[typename] }, context, info);

export const expandResolvers = {
  "Query.node": [withExpand("Node")],
  "Query.retrievePaymentIntent": [withExpand("PaymentIntent")],
  "Query.listPaymentIntent": [withExpand("PaymentIntentList")],
  "Mutation.createPaymentIntent": [withExpand("PaymentIntent")],
  "Mutation.updatePaymentIntent": [withExpand("PaymentIntent")],
  "Mutation.confirmPaymentIntent": [withExpand("PaymentIntent")],
  "Mutation.capturePaymentIntent": [withExpand("PaymentIntent")],
  "Mutation.cancelPaymentIntent": [withExpand("PaymentIntent")]
};
